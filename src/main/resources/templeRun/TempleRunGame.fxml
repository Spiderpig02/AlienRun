      <Button fx:id="startButton" layoutX="949.0" layoutY="768.0" mnemonicParsing="false" prefHeight="106.0" prefWidth="194.0" text="Start Game" textAlignment="CENTER" textFill="#09bc1e">
         <font>
            <Font name="Apple Braille" size="29.0" />
         </font>
      </Button>
      <TextField fx:id="username" layoutX="455.0" layoutY="785.0" prefHeight="60.0" prefWidth="314.0" promptText="USERNAME">
         <font>
            <Font name="Apple Braille" size="30.0" />
         </font>
      </TextField>
      <ListView fx:id="scoreboard" layoutX="900.0" prefHeight="750.0" prefWidth="300.0" />
   </children>
</AnchorPane>
